{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "jsx": "preserve",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowJs": true,
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "inngest": [
        "./node_modules/inngest/index.d.ts"
      ],
      "inngest/*": [
        "./node_modules/inngest/*"
      ]
    },
    "skipLibCheck": true,
    "noEmit": true,
    "incremental": true,
    "isolatedModules": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "exclude": [
    "node_modules",
    ".netlify",
    ".vercel"
  ],
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
